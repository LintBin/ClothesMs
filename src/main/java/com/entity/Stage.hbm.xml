<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.entity.Stage" table="t_stage">
		<id name="id" >
			<column name="id"></column>
			<generator class="native"></generator>
		</id>
		
		<property name="stageNumber" >
			<column name="stage_number" not-null="true"></column>
		</property>

		<many-to-one name="wareHouse" column="warehouse_id" cascade="none"  class="com.entity.WareHouse" not-null="true"></many-to-one>
		<many-to-one name="clothes" column="clothes_id" cascade = "none"  class="com.entity.Clothes" not-null="true"></many-to-one> 
	</class>
</hibernate-mapping>