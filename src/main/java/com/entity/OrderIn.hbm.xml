<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.entity.OrderIn" table="t_storage">
		<id name="id" >
			<column name="id"></column>
			<generator class="native"></generator>
		</id>
		
		<property name="storage_Number" >
			<column name="storage_number" not-null="true"></column>
		</property>
		
		<property name="flag">
			<column name="flag" not-null="true"></column>
		</property>
		
		<property name="date">
			<column name="in_date" not-null="true"></column>
		</property>
		
		<property name="source">
			<column name="source" not-null="true"></column>
		</property>
		
		<many-to-one name="manager" column="manager" cascade="none"  class="com.entity.Admin" not-null="true" lazy="false"></many-to-one>
		<many-to-one name="wareHouse" column="warehouse_id" cascade="none"  class="com.entity.WareHouse" not-null="true" lazy="false"></many-to-one>
	</class>
</hibernate-mapping>